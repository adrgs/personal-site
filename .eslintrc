{
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "no-underscore-dangle": ["error", {
      "allow": ["__filename", "__dirname"]
    }]
  }
}
